{
  "name": "leatherbound",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "A custom productivity homepage by Brandon Lee",
  "homepage_url": "https://github.com/dabslee/leatherbound",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": ["storage"]
}