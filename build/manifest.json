{
  "manifest_version": 3,
  "short_name": "leatherbound",
  "name": "Leatherbound New Tab",
  "version": "1.0",
  "description": "A configurable new tab extension.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
      "16": "favicon.ico",
      "48": "logo192.png",
      "128": "logo512.png"
  },
  "action": {
    "default_title": "Leatherbound"
  }
}
